<!-- ###TRANSACTOR_FORM_TEMPLATE###

This subpart is used to display the card number information dialog before the transfer to the payment gateway
-->


<div class="transactor-table">
	<div class="transactor-row">
		<div class="transactor-cell transactor-total-header">
		TOTAL:
	</div>
	</div>
	<div class="transactor-row">
		<div class="transactor-cell transactor-total-amount-header">
			Total amount in EURO:
		</div>

		<div class="transactor-cell transactor-total-amount">
			&euro; ###PRICE_TOTAL_TAX###
		</div>
	</div>
</div>

<div class="transactor-form">
	<form action="###REDIRECT_URL###" method="post" autocomplete="off" name="connectForm">
	<input type="submit" name="pay" value="Connect to the ###TRANSACTOR_TITLE### server" />&nbsp;&nbsp;
		###HIDDENFIELDS###
	</form>
</div>

<p class="important">###MESSAGE_REDIRECT###</p>

<div class="transactor-table">
	<div class="transactor-row">
		<div class="transactor-cell">
			<a href="http://###TRANSACTOR_WWW###" target="_blank">###TRANSACTOR_IMAGE###</a>
		</div>
		<div class="transactor-cell transactor-info">
			###TRANSACTOR_INFO###
		</div>
	</div>
</div>

<!-- ###TRANSACTOR_FORM_TEMPLATE###	 end -->




